<?xml version="1.0"?>

<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>VL.IO.OptiTrack</id>
    <version>0.0.1-alpha</version>
    <title>VL.IO.OptiTrack</title>
    <authors>vvvv</authors>
    <projectUrl>https://github.com/vvvv/VL.IO.OptiTrack</projectUrl>
    <repository type="git" url="https://github.com/vvvv/VL.IO.OptiTrack.git" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Support for OptiTrack in VL</description>
    <tags>VL, body, skeleton, tracking, natnet</tags>
    <license type="expression">LGPL-3.0-only</license>
    <icon>icon\nugeticon.png</icon>
    <readme>docs\README.md</readme>
    <dependencies>
      <dependency id="VL.Skeleton" version="0.0.5" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\lib\**\*.dll" target="lib\" exclude="" />
	<file src="..\runtimes\**\*.dll" target="runtimes\" exclude="" />
    <file src="..\*.vl" target="" exclude="" />
    <file src="..\help\**" target="help\" exclude="" />
    <file src="nugeticon.png" target="icon\" />
    <file src="..\README.md" target="docs\" />
  </files>
</package>
